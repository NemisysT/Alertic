generator client {
  provider = "prisma-client-py"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Report {
    id          Int      @id @default(autoincrement())
    description String
    category    String
    latitude    Float
    longitude   Float
    image       String
}